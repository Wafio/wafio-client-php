{
  "name": "wafio/wafio-client-php",
  "description": "PHP client for Wafio WAF TCP mTLS (analyze requests, check block). Feature parity with Node.js and Go clients.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Wafio",
      "homepage": "https://wafio.cloud"
    }
  ],
  "keywords": [
    "waf",
    "wafio",
    "mtls",
    "security",
    "firewall",
    "web-application-firewall",
    "sqli",
    "xss",
    "owasp",
    "tcp",
    "tls"
  ],
  "homepage": "https://github.com/Wafio/wafio-client-php",
  "support": {
    "issues": "https://github.com/Wafio/wafio-client-php/issues",
    "source": "https://github.com/Wafio/wafio-client-php"
  },
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "ext-openssl": "*"
  },
  "autoload": {
    "psr-4": {
      "Wafio\\Client\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
